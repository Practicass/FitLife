
const {Schema, model} = require("mongoose")


const rutineSchema = Schema({

    name: {
        type: String,
        required: true
    },
    user: {
        type: Schema.ObjectId,
        ref: "User"
    },
    exercises: [{
        type: Schema.ObjectId,
        ref: "Exercise"
    }]

    
})

module.exports = model("Rutine", rutineSchema, "exercises")